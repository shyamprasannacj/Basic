#include<bits/stdc++.h>
using namespace std;
 
void solve(){
		string s;
		cin>>s;
		for (char c:s){
			if (c<'a'){
				c=c+32;
 
			}
			
			if (!(c=='a' || c=='e'|| c=='i'||c=='o'||c=='u'||c=='y')){
				cout<<"."<<c;
			}
		}
}
 
 
int main(){
 
	#ifndef ONLINE_JUDGE
	     freopen("input.txt", "r", stdin);
   		 freopen("output.txt", "w", stdout);
	#endif
   ios::sync_with_stdio(0);
   cin.tie(0);
   solve(); 
   return 0;
}
