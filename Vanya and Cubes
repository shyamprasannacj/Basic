#include<bits/stdc++.h>
using namespace std;
int main(){
 
	#ifndef ONLINE_JUDGE
    freopen("input.txt", "r", stdin);
    freopen("output.txt", "w", stdout);
	#endif
 
	int n,total,i=1,x=0;
	cin>>n;
	while(true){
		if (x+(i*(i+1)/2)>n) break;
		x+=(i*(i+1))/2;
		i++;
		//cout<<x<<endl;
	}
	cout<<i-1;
	return 0;
}
