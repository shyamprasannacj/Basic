#include<bits/stdc++.h>
using namespace std;
int main(){
 
	#ifndef ONLINE_JUDGE
    freopen("input.txt", "r", stdin);
    freopen("output.txt", "w", stdout);
	#endif
 
	string s,t;
	cin>>s;
	int flag=1;
	for (int i=0;i<5;i++){
		cin>>t;
		if (s[0]==t[0]||s[1]==t[1]) flag=0;
	}		
	if (flag) cout<<"NO";
	else cout<<"YES";
 
}
