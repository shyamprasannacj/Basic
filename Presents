#include<bits/stdc++.h>
using namespace std;
int main(){
	#ifndef ONLINE_JUDGE
    freopen("input.txt", "r", stdin);
    freopen("output.txt", "w", stdout);
	#endif
 
	unordered_map<int,int>m;
	int n,val;
	cin>>n;
	for (int i=1;i<=n;i++) {
		cin>>val;
		m[val]=i;
	}
	for (int i=1;i<=n;i++) cout<<m[i]<<" ";
	return 0;
}
